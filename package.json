{
  "name": "reactwebpack1",
  "version": "1.0.0",
  "description": "boilerplate webpack ts react app",
  "private": true,
  "scripts": {
    "start": "cd build && webpack serve",
    "dev": "webpack serve --port 3000 --host 0.0.0.0 --config ./config/webpack/webpack.dev.config.ts",
    "devlocal": "webpack serve --port 3005 --config ./config/webpack/webpack.dev.config.ts",
    "build": "webpack --config ./config/webpack/webpack.prod.config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "linter": "eslint './src/**/*.{ts,tsx}'",
    "linter:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "stats": "webpack --profile --json > stats.json",
    "build-report": "webpack-bundle-analyzer --port 4200 build/statsgemini.json"
  },
  "keywords": [],
  "author": "gemini stars",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/dotenv-webpack": "^5.0.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/redux-thunk": "^2.1.0",
    "@types/reselect": "^2.2.0",
    "@types/webpack": "^4.41.26",
    "@types/webpack-bundle-analyzer": "^3.9.1",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "dotenv-webpack": "^6.0.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.9",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "reselect": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "redux": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.(ts|tsx)": [
      "npm run --silent lint:fix"
    ]
  }
}
